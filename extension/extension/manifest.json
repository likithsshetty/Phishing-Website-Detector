{
  "manifest_version": 2,
  "name": "Pish Detector",
  "version": "1.0",
  "description": "Detects phishing attempts and enhances security.",
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "tabs"
  ],
  "host_permissions": ["<all_urls>"]
}
